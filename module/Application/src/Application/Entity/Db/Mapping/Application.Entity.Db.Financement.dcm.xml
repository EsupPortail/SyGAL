<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Application\Entity\Db\OrigineFinancement" table="ORIGINE_FINANCEMENT"
            repository-class="">
        <id name="id" type="integer" column="ID">
            <generator strategy="SEQUENCE"/>
        </id>

        <many-to-one field="source" target-entity="UnicaenImport\Entity\Db\Source">
            <join-columns>
                <join-column name="SOURCE_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>

        <many-to-one field="these" target-entity="Application\Entity\Db\These">
            <join-columns>
                <join-column name="THESE_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>

        <field name="annee"                     column="ANNEE_ID"                   type="int"                          nullable="false" />

        <many-to-one field="origineFinacement" target-entity="Application\Entity\Db\OrigineFinancement">
            <join-columns>
                <join-column name="ORIGINE_FINANCEMENT_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>

        <field name="complementFinancement"     column="COMPLEMENT_FINANCEMENT"     type="string"   length="256"        nullable="true" />
        <field name="quotiteFinancement"        column="QUOTITE_FINANCEMENT"        type="string"   length="8"          nullable="true" />
        <field name="dateDebut"                 column="DATE_DEBUT"                 type="datetime"                     nullable="true" />
        <field name="dateFin"                   column="DATE_FIN"                   type="datetime"                     nullable="true" />

    </entity>
</doctrine-mapping>